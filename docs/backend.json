{
  "entities": {
    "BudgetResult": {
      "title": "BudgetResult",
      "type": "object",
      "description": "Represents the final, processed output of a successful analysis run.",
      "properties": {
        "userId": {
          "type": "string",
          "description": "The Firebase Auth UID of the user who initiated the processing."
        },
        "uploadId": {
          "type": "string",
          "description": "Reference to the document in the 'uploads' collection."
        },
        "timestamp": {
          "type": "string",
          "format": "date-time",
          "description": "Timestamp indicating when the processing was completed."
        },
        "retailRevenue": {
          "type": "number",
          "description": "Retail revenue for the period."
        },
        "wholesaleRevenue": {
          "type": "number",
          "description": "Wholesale revenue for the period."
        },
        "totalCosts": {
          "type": "number",
          "description": "Total costs for the period."
        },
        "costsByHolder": {
          "type": "object",
          "description": "A key-value map where the key is the budget holder and the value is the total cost.",
          "additionalProperties": {
            "type": "number"
          }
        },
        "costsByRegion": {
          "type": "object",
          "description": "A key-value map where the key is the region and the value is the total cost.",
          "additionalProperties": {
            "type": "number"
          }
        },
        "anomalies": {
          "type": "array",
          "description": "Array of AI-generated anomaly descriptions.",
          "items": {
            "type": "string"
          }
        },
        "geminiPrompt": {
          "type": "string",
          "description": "Optional, for debugging. The exact prompt sent to Gemini for revenue classification."
        }
      },
      "required": [
        "userId",
        "uploadId",
        "timestamp",
        "retailRevenue",
        "wholesaleRevenue",
        "totalCosts",
        "costsByHolder"
      ]
    },
    "UserProfile": {
      "title": "UserProfile",
      "type": "object",
      "description": "Represents a user profile within the application.",
      "properties": {
        "email": {
          "type": "string",
          "description": "User's email address.",
          "format": "email"
        },
        "role": {
          "type": "string",
          "enum": [
            "admin",
            "viewer"
          ],
          "description": "User's role (admin or viewer)."
        },
        "createdAt": {
          "type": "string",
          "format": "date-time",
          "description": "Timestamp of when the user was created."
        }
      },
      "required": [
        "email",
        "role",
        "createdAt"
      ]
    },
    "UploadSession": {
      "title": "UploadSession",
      "type": "object",
      "description": "Represents a file upload session and its processing status.",
      "properties": {
        "userId": {
          "type": "string",
          "description": "The Firebase Auth UID of the user who initiated the session."
        },
        "sessionId": {
          "type": "string",
          "description": "Unique ID for the upload session."
        },
        "createdAt": {
          "type": "string",
          "format": "date-time",
          "description": "Timestamp when the session was created."
        },
        "readyAt": {
          "type": "string",
          "format": "date-time",
          "description": "Timestamp when all files were uploaded and ready for processing."
        },
        "status": {
          "type": "string",
          "enum": [
            "uploading",
            "ready_for_processing",
            "processing",
            "completed",
            "error"
          ],
          "description": "The processing status of the upload session."
        },
        "files": {
          "type": "object",
          "description": "Map of uploaded files and their details.",
          "properties": {
            "glEntries": { "$ref": "#/entities/FileDetails" },
            "budgetHolderMapping": { "$ref": "#/entities/FileDetails" },
            "costItemMap": { "$ref": "#/entities/FileDetails" },
            "regionalMapping": { "$ref": "#/entities/FileDetails" },
            "corrections": { "$ref": "#/entities/FileDetails" },
            "revenueReport": { "$ref": "#/entities/FileDetails" }
          }
        },
        "errorMessage": {
          "type": "string",
          "description": "If status is 'error', this field contains the reason."
        },
        "resultId": {
          "type": "string",
          "description": "The ID of the corresponding document in the budget_results collection."
        }
      },
      "required": ["userId", "sessionId", "createdAt", "status", "files"]
    },
    "FileDetails": {
      "title": "FileDetails",
      "type": "object",
      "properties": {
        "name": { "type": "string" },
        "path": { "type": "string" },
        "uploadedAt": { "type": "string", "format": "date-time" }
      },
      "required": ["name", "path", "uploadedAt"]
    }
  },
  "auth": {
    "providers": [
      "google.com"
    ]
  },
  "firestore": {
    "structure": [
      {
        "path": "/users/{userId}",
        "definition": {
          "schema": {
            "$ref": "#/entities/UserProfile"
          },
          "description": "Stores user profile information, with the document ID matching the user's Firebase Auth UID."
        }
      },
      {
        "path": "/upload_sessions/{sessionId}",
        "definition": {
          "schema": {
            "$ref": "#/entities/UploadSession"
          },
          "description": "Tracks the state and metadata of a multi-file upload job."
        }
      },
      {
        "path": "/budget_results/{resultId}",
        "definition": {
          "schema": {
            "$ref": "#/entities/BudgetResult"
          },
          "description": "Stores the final, processed output of a successful analysis run."
        }
      }
    ]
  }
}
